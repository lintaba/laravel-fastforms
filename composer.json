{
    "name": "lintaba/laravel-fastforms",
    "description": "Fast form definition for laravel models with validation and views",
    "type": "library",
    "authors": [
        {
            "name": "lintaba",
            "email": "lintaba@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "illuminate/support": ">=5.0 <8.0",
        "illuminate/session": ">=5.0 <8.0",
        "laravelcollective/html": ">=5.7"

    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "Lintaba\\Fastforms\\":"src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Lintaba\\Fastforms\\FastformsServiceProvider"
            ],
            "aliases": {}
        }
    },
    "suggest": {
        "barryvdh/laravel-translation-manager": "Collects untranslated (autogenerated) keys",
        "barryvdh/laravel-debugbar": "Helps debug unexpected missing form fillings"
    }

}
